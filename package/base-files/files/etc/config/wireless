
config wifi-device 'radio0'
	option type 'ralink'
	option variant 'mt7628'
	option country 'CN'
	option hwmode '11bgn'
	option htmode 'HT40'
	option channel 'auto'
	option disabled '0'

config wifi-iface 'ap'
	option device 'radio0'
	option mode 'ap'
	option network 'lan'
	option ifname 'ra0'
	option hidden '0'
	option encryption 'psk2'
	option key '01010101'
	option ssid 'MC-000000'

config wifi-iface 'sta'
	option device 'radio0'
	option mode 'sta'
	option network 'wlan'
	option ifname 'apcli0'
	option ssid 'MC-999999'
	option key '01010101'

